{
    "schemaVersion": 1,
    "id": "enderscape",
    "version": "${version}",
    "name": "Enderscape",
    "description": "A mod that adds new content to The End dimension, such as biomes, mobs, ores, and more.\nThe focus is to add features that aren't unfitting for the vanilla game while enhancing the overall experience.",
    "authors": [
        "bunten (Primary developer)"
    ],
    "contributors": [
        "Aigis (3D Logo)",
        "ArathainFarqoe (Additional programming and art)",
        "FourTimesCold (Concept art)",
        "MaakuArc (3D art and concept art)"
    ],
    "contact": {
        "sources": "https://github.com/bunten2005/enderscape",
        "issues": "https://github.com/bunten2005/enderscape/issues"
    },
    "custom": {
        "modmenu": {
            "links": {
                "modmenu.enderscape.curseforge": "https://www.curseforge.com/minecraft/mc-mods/enderscape",
                "modmenu.enderscape.modrinth": "https://modrinth.com/mod/enderscape"
            }
        }
    },
    "license": "MIT",
    "icon": "assets/enderscape/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "net.bunten.enderscape.Enderscape"
        ],
        "client": [
            "net.bunten.enderscape.client.EnderscapeClient"
        ]
    },
    "mixins": [
        "enderscape.mixins.json",
        "enderscape.client.mixins.json"
    ],
    "depends": {
        "minecraft": "1.18.2",
        "fabricloader": ">=0.13.3",
        "fabric": ">=0.51.1",
        "bclib": ">=1.4.4"
    },
    "accessWidener": "enderscape.accesswidener"
}